{
  "presets": [
      ["env", { "modules": false }],
      "stage-0", "es2015"
  ],
  "plugins": [
      "transform-runtime",
      "async-to-promises"
  ],
  "env": {
      "test": {
          "plugins": [
              "transform-es2015-modules-commonjs",
              "dynamic-import-node"
          ]
      }
  }
}
